<!--miniprogram/kubernetes/index.wxml-->
<view class="background-view">
  <van-cell-group title="Kubernetes Cheat Sheet">
    <block wx:for="{{catalogs}}" wx:key="_id">
      <van-cell title="{{item.name}}" is-link value="详细" />
    </block>
  </van-cell-group>
  <view style="margin: 10rpx;"></view>
  <cheatcard desc="{{desc}}" descCN="{{descCN}}" codeText="{{codeText}}" avatar="{{avatar}}"
    contributor="{{contributor}}" source="{{source}}"></cheatcard>
</view>